    cat << END                                            >> $NJET_MAKEFILE

$NJET_LIBATOMIC/src/libatomic_ops.a:	$NJET_LIBATOMIC/Makefile
	cd $NJET_LIBATOMIC && \$(MAKE)

$NJET_LIBATOMIC/Makefile:	$NJET_MAKEFILE
	cd $NJET_LIBATOMIC \\
	&& if [ -f Makefile ]; then \$(MAKE) distclean; fi \\
	&& ./configure

END
