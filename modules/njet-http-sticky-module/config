njt_feature="njt_http_stick_module"
njt_feature_name=
njt_feature_run=no
njt_feature_incs=
njt_feature_libs=""
njt_feature_path="$njt_addon_dir"
njt_feature_deps=
njt_check_src="\
    $njt_addon_dir/src/njt_http_sticky_module.c \
    $njt_addon_dir/src/njt_http_sticky_cookie.c \
    $njt_addon_dir/src/njt_http_sticky_learn.c \
    $njt_addon_dir/src/njt_http_sticky_route.c \
"

njt_feature_test="int a;"
. auto/feature

if [ $njt_found = yes ]; then
    have=NJT_HTTP_STICKY . auto/have
    CORE_INCS="$CORE_INCS $njt_feature_path"
    njt_addon_name=njt_http_sticky_module
    njt_module_type=HTTP
    njt_module_name=$njt_addon_name
    HTTP_MODULES="$HTTP_MODULES njt_http_sticky_module"
    NJT_ADDON_DEPS="$NJT_ADDON_DEPS $njt_feature_deps"
    NJT_ADDON_SRCS="$NJT_ADDON_SRCS $njt_check_src"
else
    cat << END
    $0: error: the njt_http_sticky_module addon error.
END
    exit 1
fi
