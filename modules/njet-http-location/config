njt_feature_name=
njt_feature_run=no
njt_feature_incs=
njt_feature_libs=
njt_feature_path="$njt_addon_dir"
njt_check_src="$njt_addon_dir/src/njet-http-location.c"


njt_feature_deps=""

njt_feature_test="int a;"
. auto/feature

if [ $njt_found = yes ]; then
    have=NJT_HTTP_DYNAMIC_LOC . auto/have
    CORE_INCS="$CORE_INCS $njt_feature_path"
    njt_addon_name=njt_http_location_module
    njt_module_type=HTTP
    njt_module_name=$njt_addon_name
    HTTP_MODULES="$HTTP_MODULES $njt_addon_name"
    NJT_ADDON_DEPS="$NJT_ADDON_DEPS $njt_feature_deps"
    NJT_ADDON_SRCS="$NJT_ADDON_SRCS $njt_check_src"
else
    cat << END
    $0: error: the njt_http_location_module  addon error.
END
    exit 1
fi
